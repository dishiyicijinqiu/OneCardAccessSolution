<dxr:DXRibbonWindow
    x:Class="FengSharp.OneCardAccess.Client.PC.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon"
    xmlns:View="clr-namespace:FengSharp.OneCardAccess.Client.PC.View" 
    xmlns:res="clr-namespace:FengSharp.OneCardAccess.Client.PC.Properties"
    
    Title="{x:Static res:Resources.View_MainWindow_Title}" 
  SizeToContent="WidthAndHeight"
    WindowStartupLocation="CenterScreen" 
    Opacity="100" 
    Background="Transparent"
    >
        <View:MainView x:Name="mainview"/>

</dxr:DXRibbonWindow>
