<dxr:DXRibbonWindow
    x:Class="FengSharp.OneCardAccess.Client.PC.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon"
    xmlns:View="clr-namespace:FengSharp.OneCardAccess.Client.PC.View" 
    xmlns:res="clr-namespace:FengSharp.OneCardAccess.Client.PC.Properties"
    WindowStartupLocation="CenterScreen" WindowState="Maximized"
    Opacity="0" AllowsTransparency="True" WindowStyle="None"
    Title="{x:Static res:Resources.View_MainWindow_Title}" 
    MinWidth="800" ShowInTaskbar="False"
    MinHeight="600"
    >
    <View:MainView />
</dxr:DXRibbonWindow>
