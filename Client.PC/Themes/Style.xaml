        <Setter Property="LargeGlyph" Value="/FengSharp.OneCardAccess.Client.PC;component/Resources/Bars/copy-32x32.png"/>
    </Style>
    <Style TargetType="dxb:BarButtonItem" x:Key="barEdit">
