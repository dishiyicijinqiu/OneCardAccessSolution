<dxr:DXRibbonWindow
    x:Class="FengSharp.OneCardAccess.Client.PC.UI.BaseRibbonWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
    xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon"
    xmlns:core="clr-namespace:FengSharp.OneCardAccess.Core"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" 
    >
    <!--<dxr:DXRibbonWindow.DataContext>
        <core:BaseWindowVM/>
    </dxr:DXRibbonWindow.DataContext>-->
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Closed">
            <i:InvokeCommandAction Command="{Binding CloseCommand, Mode=OneWay}" />
        </i:EventTrigger>
    </i:Interaction.Triggers>
</dxr:DXRibbonWindow>
