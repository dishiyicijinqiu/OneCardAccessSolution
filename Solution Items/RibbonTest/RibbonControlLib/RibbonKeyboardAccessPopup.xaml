    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:src="clr-namespace:DNBSoft.WPF.RibbonControl"
    Height="16" MinWidth="14">
    <Grid>
        <src:RibbonBorder BorderThickness="0.5" BorderBrush="Black" CornerRadius="1" x:Name="theBorder">
            <Label Name="keyLabel" FontSize="12" Margin="-3,-6,-3,-6" HorizontalAlignment="Center">
                
            </Label>
        </src:RibbonBorder>
    </Grid>
</UserControl>
